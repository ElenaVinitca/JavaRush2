public class Solution {                                                                                                                                                                                                                                                                                                                                                              
    public static Map<String, Date> createMap() throws ParseException {                                                                                                                                                                                                                                                                                                                                                              
        DateFormat dateFormat = new SimpleDateFormat("MMMMM d yyyy", Locale.ENGLISH);                                                                                                                                                                                                                                                                                                                                                              
        Map<String, Date> map = new HashMap<>();                                                                                                                                                                                                                                                                                                                                                              
        map.put("Сталлоне", dateFormat.parse("MAY 1 2012"));                                                                                                                                                                                                                                                                                                                                                              
        map.put("Familia", dateFormat.parse("APRIL 1 2019"));                                                                                                                                                                                                                                                                                                           
        map.put("Familia1", dateFormat.parse("AUGUST 1 2019"));                                                                                                                                                                                                                                                                                                         
        map.put("Familia2", dateFormat.parse("DECEMBER 1 2019"));                                                                                                                                                                                                                                                                                                        
        map.put("Familia3", dateFormat.parse("FEBRUARY 1 2019"));                                                                                                                                                                                                                                                                                                      
        map.put("Familia4", dateFormat.parse("JANUARY 1 2019"));                                                                                                                                                                                                                                                                                                        
        map.put("Familia5", dateFormat.parse("JULY 1 2019"));                                                                                                                                                                                                                                                                                                     
        map.put("Familia6", dateFormat.parse("JUNE 1 2019"));                                                                                                                                                                                                                                                                                                     
        map.put("Familia7", dateFormat.parse("MARCH 1 2019"));                                                                                                                                                                                                                                                                                                                                             
        map.put("Familia8", dateFormat.parse("MAY 1 2019")); //напишите тут ваш код                                                                                                                                                                                                                                                                                                                                                              
           return map;                                                                                                                                                                                                
 }                                                                                                                                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                                                                                                                              
    public static void removeAllSummerPeople(Map<String, Date> map) {                                                                                                                                                                                                                                                                                                                                                              
        //напишите тут ваш код                                                                                                                                                                                                                                                                                                                                                              
       //Map<String, Date> map2 = new HashMap<>();                                                                                                    
         Iterator<Map.Entry<String, Date>> pair = map.entrySet().iterator();                                                  
           while(pair.hasNext()){                                                  
           Map.Entry<String, Date> pair2 = pair.next();  
             Date date = pair2.getValue(); 
              int month = date.getMonth();                   
             if(month == 5 || month == 6 || month == 7)                                                   
                pair.remove();                                                                                                                                                                                                                                                                                          
    }                                                                                                                                                                                                                                                                                                                                                              
   }                                                                                                                                                                                                                                                                                                                                                           
    public static void main(String[] args) {                                                                                                                                                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                                                                                                                              
    }                                                                                                                                                                                                                                                                                                                                                              
}                         
